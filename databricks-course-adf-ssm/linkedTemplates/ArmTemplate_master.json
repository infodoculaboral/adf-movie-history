{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "databricks-course-adf-ssm"
		},
		"ls_movie_history_adls_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"ls_movie_history_adls\""
		},
		"LS_Databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1021-073915-qjp0tuyo"
		},
		"ls_movie_history_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mymoviehistory.dfs.core.windows.net/"
		},
		"tg_proccess_movie_history_properties_pl_proccess_movie_history_parameters_p_environment": {
			"type": "string",
			"defaultValue": "Production"
		},
		"tg_proccess_movie_history_properties_pl_proccess_movie_history_parameters_p_file_date": {
			"type": "string",
			"defaultValue": "2024-12-30"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databricks-course-adf-ssm_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_movie_history_adls_accountKey": {
						"value": "[parameters('ls_movie_history_adls_accountKey')]"
					},
					"LS_Databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('LS_Databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_movie_history_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_movie_history_adls_properties_typeProperties_url')]"
					},
					"tg_proccess_movie_history_properties_pl_proccess_movie_history_parameters_p_environment": {
						"value": "[parameters('tg_proccess_movie_history_properties_pl_proccess_movie_history_parameters_p_environment')]"
					},
					"tg_proccess_movie_history_properties_pl_proccess_movie_history_parameters_p_file_date": {
						"value": "[parameters('tg_proccess_movie_history_properties_pl_proccess_movie_history_parameters_p_file_date')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}